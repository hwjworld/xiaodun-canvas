<header class="entry-header discussion-section admin-link-hover-area {{read_state}} clearfix {{#if collapsable}}collapsable{{/if}}" {{#if collapsable}}data-event="toggleCollapsed"{{/if}}>
  <div class="discussion-header-content right-of-avatar">
    <div class="discussion-header-right">
      <div class="headerBadges show-if-collapsed"></div>
    </div>
    <div class="discussion-title">{{#t "deleted"}} 此帖已被 <a {{editor_href}} tabindex="0">{{editor_name}}</a> 删除{{/t}}</div>
  </div>
</header>

